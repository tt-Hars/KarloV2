{
  "name": "logging",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/shared/logging/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/modules/shared/logging",
        "main": "modules/shared/logging/src/index.ts",
        "tsConfig": "modules/shared/logging/tsconfig.lib.json",
        "assets": ["modules/shared/logging/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "modules/shared/logging/jest.config.cts"
      }
    }
  }
}

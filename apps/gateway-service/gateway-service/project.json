{
  "name": "gateway-service-gateway-service",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/gateway-service/gateway-service/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "gateway-service-gateway-service:build",
        "inspect": false,
        "args": ["--inspect=0.0.0.0:9230"]
      },
      "configurations": {
        "development": {
          "buildTarget": "gateway-service-gateway-service:build:development"
        },
        "production": {
          "buildTarget": "gateway-service-gateway-service:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/gateway-service/gateway-service/jest.config.ts"
      }
    }
  }
}

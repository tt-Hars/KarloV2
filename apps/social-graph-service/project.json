{
  "name": "social-graph-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/social-graph-service/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/social-graph-service/package.json",
        "{workspaceRoot}/dist/apps/social-graph-service/pnpm-lock.yaml"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": ["build"],
      "cache": true,
      "outputs": [
        "{workspaceRoot}/dist/apps/social-graph-service/workspace_modules"
      ],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": ["prune-lockfile", "copy-workspace-modules"],
      "executor": "nx:noop"
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/social-graph-service",
        "main": "apps/social-graph-service/src/main.ts",
        "tsConfig": "apps/social-graph-service/tsconfig.app.json",
        "assets": ["apps/social-graph-service/src/assets"],
        "webpackConfig": "apps/social-graph-service/webpack.config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "inspect": false
        },
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/social-graph-service/src/environments/environment.ts",
              "with": "apps/social-graph-service/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "social-graph-service:build",
        "runBuildTargetDependencies": false,
        "inspect": false,
        "args": ["--inspect=0.0.0.0:9233"]
      },
      "configurations": {
        "development": {
          "buildTarget": "social-graph-service:build:development"
        },
        "production": {
          "buildTarget": "social-graph-service:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/social-graph-service/jest.config.cts",
        "passWithNoTests": true
      }
    }
  }
}
